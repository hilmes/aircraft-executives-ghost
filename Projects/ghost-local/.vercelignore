*.log
*.pid
*.db

# Node modules (will be installed by Vercel)
node_modules/

# Development files
.env*
*.local

# OS files
.DS_Store
Thumbs.db

# IDE files
.vscode/
.idea/

# Git files
.git/

# Keep only essential files: api/, package.json, vercel.json, README.md