{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/ghost/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_SQLITE": "true",
    "url": "https://aircraftexecutives.vercel.app",
    "server__host": "0.0.0.0",
    "server__port": "2368"
  }
}